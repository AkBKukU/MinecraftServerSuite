_mcmanager_complete()
{	
    local COMPLETES="getNewLink getNewestVersion getCurrentVersion checkForUpdate autoUpdate updateMinecraft installMinecraft setOPS setMOTD backupMinecraft autoBackupMinecraft updatemcManager status"

    # we put the completions into $COMPREPLY using compgen
    COMPREPLY=( $(compgen -W "$COMPLETES" -- ${COMP_WORDS[COMP_CWORD]}) )
    return 0
}

complete -F _mcmanager_complete mcmanager 